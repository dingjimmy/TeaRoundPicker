{
  "name": "asp.net",
  "version": "1.0.0",
  "private": true,
  "devDependencies": {
    "del-cli": "4.0.1",
    "typescript": "^4.5.4",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "uglify-js": "3.14.5"
  },
  "scripts": {
    "build": "tsc --build",
    "clean": "del-cli scripts/out/**/* && del-cli wwwroot/scripts/**/*",
    "publish": "tsc --outfile scripts/out/app.js && uglifyjs scripts/out/app.js --output wwwroot/scripts/app.js",
    "rebuild": "npm run clean && npm run build"
  },
  "-vs-binding": {
    "Clean": [
      "clean"
    ]
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
