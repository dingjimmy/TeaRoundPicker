{
  "compilerOptions": {
    "target": "ES6",
    "noImplicitAny": true,
    "noEmitOnError": true,
    "removeComments": true,
    "sourceMap": true,
    "module": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "jsx": "react",
    "outDir": "wwwroot/scripts"
  },
  "include": [
    "scripts"
  ]
}
